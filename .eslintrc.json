{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-use-before-define": 0,
    "react/jsx-no-comment-textnodes": 0,
    "react/prefer-stateless-function": 0
  },
  "parser": "babel-eslint"
}
